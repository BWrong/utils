{
  "name": "@bwrong/auth-tool",
  "version": "1.2.8",
  "author": "bwrong<ibwrong@foxmail.com>",
  "license": "MIT",
  "keywords": [
    "auth",
    "vue"
  ],
  "description": "vue项目资源（菜单、路由、按钮...）权限控制解决方案",
  "main": "lib/index.js",
  "module": "lib/index.esm.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "rollup -c",
    "release": "standard-version --infile CHANGELOG.md && npm run build"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/BWrong/auth-tool.git",
  "bugs": {
    "url": "https://github.com/BWrong/auth-tool.git/issues",
    "email": "ibwrong@foxmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BWrong/auth-tool.git"
  },
  "engines": {
    "node": ">=12"
  },
  "files": [
    "lib/*"
  ],
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-eslint": "^8.0.1",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "rollup": "^2.35.1",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "standard-version": "^9.0.0",
    "tslib": "^1.14.1",
    "typescript": "^4.3.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "standard-version": {
    "skip": {
      "commit": false
    }
  },
  "peerDependencies": {
    "vue": "^3.0.11",
    "vue-router": "^4.0.8"
  }
}
